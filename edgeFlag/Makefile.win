.PHONY: all all-before all-after clean clean-custom

all: all-before edgeFlag.exe all-after


clean: clean-custom
	${RM} $(OBJ) $(BIN)

$(BIN): $(OBJ)
	$(CPP) $(LINKOBJ) -o "edgeFlag.exe" $(LIBS)

edgeFlag.o: edgeFlag.cpp
	$(CPP) -c edgeFlag.cpp -o edgeFlag.o $(CXXFLAGS)
